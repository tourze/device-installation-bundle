{
  "name": "tourze/device-installation-bundle",
  "description": "安装",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^3.7.0 || ^4.0",
    "doctrine/persistence": "^3.1 || ^4",
    "doctrine/orm": "^2.20 || ^3.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/device-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "DeviceInstallationBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DeviceInstallationBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}